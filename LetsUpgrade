// Find max value in array
function findMax(numbers) {
  let max = numbers[0];

  for (let i = 1; i < numbers.length; i++) {
    if (numbers[i] > max) {
      max = numbers[i];
    }
  }

  return max;
}
// Example usage
const numbers = [5, 10, 25, 6]; 

const maxNum = findMax(numbers);

console.log(maxNum); // Outputs 25
